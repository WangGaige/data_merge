    "import pandas as pd\n",
    "file1='./2018.xls'\n",
    "file2='./2019.xls'\n",
    "file3='./2020.xls'\n",
    "file=[file1,file2,file3]\n",
    "li=[]\n",
    "for i in file:\n",
    "  li.append(pd.read_excel(i))\n",
    "writer = pd.ExcelWriter('./merged.xls')\n",
    "pd.concat(li).to_excel(writer,'油井',index=False)\n",
    "  \n",
    "writer.save()"
